	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx("EvidenceCheckerBlueSkull", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
	Exist:
		BSKU A 10
		BSKU B 10 BRIGHT
		Loop
	PickUp:
		TNT1 A 1 A_GiveInventory("BlueSkull",1)
		TNT1 A 1 ACS_NamedExecuteAlways("GiveAllPlayersBlueSkull", 0)
		Stop
